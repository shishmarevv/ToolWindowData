# Example environment configuration
# Copy this file to .env and adjust as needed

# Path to CSV file
CSV_PATH=/app/data/toolwindow_data.csv

# Path to database
DB_PATH=/app/database/toolwindow.db

# Maximum duration threshold in minutes (default: 12 hours)
MAX_DURATION=720

# Number of worker threads
WORKERS_COUNT=4

# Batch size for user processing
USER_BATCH_SIZE=100

# Batch size for database operations
BATCH_SIZE=100

# Logging level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

